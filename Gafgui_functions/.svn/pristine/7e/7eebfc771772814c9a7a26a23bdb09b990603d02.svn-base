function [err,ISOD,DOSERANGE,TAURANGE,pr,pg,pb,pwr,pwg,pwb] = fct_ReadMultichCorr(fname)
err = 1;
file = fopen(fname,'r');
A = fscanf(file,'%e',[1 inf]);
ISOD = A(1);
DOSERANGE = A(2:3); DOSERANGE = DOSERANGE(:);
TAURANGE = A(4:5); TAURANGE = TAURANGE(:);
n1 = A(6);
n2 = A(7); 
i = 7+(1:n1);
pr = A(i);  pr = pr(:);
i = max(i)+(1:n1);
pg = A(i);  pg = pg(:);
i = max(i)+(1:n1);
pb = A(i);  pb = pb(:);
i = max(i)+(1:n2);
pwr = A(i); pwr = pwr(:); 
i = max(i)+(1:n2);
pwg = A(i); pwg = pwg(:);
i = max(i)+(1:n2);
pwb = A(i); pwb = pwb(:);   
fclose(file);
err = 0;