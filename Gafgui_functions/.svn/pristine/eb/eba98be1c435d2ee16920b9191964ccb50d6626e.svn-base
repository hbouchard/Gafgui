function asol = fct_solve_abc(theta)


n = length(theta);
mu = mean(theta);
sigma = sqrt(n/(n-1)*mean((theta-mu).^2));
%reduced variable
z = (theta-mu)/sigma;
%unbiased skewness
gamma3 = ( n^2/(n-2)/(n-1) *mean(z.^3));
% %unbiased kurtosis
% kappa4 = ((n+1)*n^2/(n-1)/(n-2)/(n-3)*mean(z.^4) - 3*((n-1)^2/(n-2)/(n-3)-1));


bsol = sigma;
csol = gamma3*sigma/6;
asol = mu-csol;

psol_approx = [bsol csol];

f1 = @(p)(p(1).^2 +2*p(2).^2 - sigma^2 )/sigma^2;
f2 = @(p) (6*p(1).^2.*p(2) + 8*p(2).^3 - gamma3*sigma^3)/sigma^3;

F = @(p) [ f1(p);
           f2(p);];
       
p = fsolve(F,psol_approx);

asol = [(mu-p(2)) p(1) p(2)];